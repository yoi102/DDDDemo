{
  "AppSettings": {
    "Token": "tttttt"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
      "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
      "WriteTo": [
        { "Name": "Console" },
        {
          "Name": "File",
          "Args": {
            "path": "./logs/myLog-.txt",
            "rollingInterval": "Day"
          }
        }
      ]
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
      //SqlServer ConnectionStrings
      "DefaultConnection": "Data Source=IRA;Initial Catalog=GameManagerDemoDB;Integrated Security=True;Encrypt=True;Trusted_Connection=True;TrustServerCertificate=True;"
      //"DefaultConnection": "Data Source=(localdb)\\mssqllocaldb;Initial Catalog=GameManagerDemoDB;Integrated Security=True;Encrypt=True;Trusted_Connection=True;TrustServerCertificate=True;"
      //"DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=GameManagerDemoDB;Trusted_Connection=True;MultipleActiveResultSets=true"

    }


